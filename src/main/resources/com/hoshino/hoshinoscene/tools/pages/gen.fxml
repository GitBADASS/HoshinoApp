<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="9.0" minWidth="300.0" prefHeight="185.0" prefWidth="400.0" stylesheets="@../../stylesheet/normal.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hoshino.hoshinoscene.tools.GenController">
   <VBox prefWidth="400.0" VBox.vgrow="ALWAYS">
      <children>
         <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
            <children>
               <Label text="填写英语" />
                <TextField fx:id="en" styleClass="wordsInput" />
               <Button fx:id="clearEn" mnemonicParsing="false" text="清空" />
            </children>
         </HBox>
         <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
            <children>
               <Label text="填写中文" />
                <TextField fx:id="cn" prefWidth="161.0" styleClass="wordsInput" />
               <Button fx:id="clearCn" mnemonicParsing="false" text="清空" />
            </children>
         </HBox>
         <HBox alignment="CENTER" prefHeight="76.0" prefWidth="400.0">
            <children>
               <HBox alignment="CENTER_LEFT" prefWidth="200.0">
                  <children>
                     <Button mnemonicParsing="false" prefWidth="65.0" text="取消" />
                  </children>
                  <padding>
                     <Insets left="10.0" />
                  </padding>
               </HBox>
               <HBox alignment="CENTER_RIGHT" prefWidth="200.0">
                  <children>
                     <Button mnemonicParsing="false" text="下一个" />
                     <Button mnemonicParsing="false" text="保存" />
                  </children>
                  <HBox.margin>
                     <Insets />
                  </HBox.margin>
                  <padding>
                     <Insets right="10.0" />
                  </padding>
               </HBox>
            </children>
         </HBox>
      </children>
   </VBox>
</VBox>
