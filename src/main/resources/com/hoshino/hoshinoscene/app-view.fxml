<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" minHeight="600.0" minWidth="1000.0" styleClass="main" stylesheets="@stylesheet/normal.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hoshino.hoshinoscene.Controller">
   <fx:define>
   </fx:define>
   <HBox fx:id="contentHBox" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
      <children>
         <FlowPane fx:id="content" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
         <VBox prefHeight="200.0" prefWidth="120.0" style="-fx-background-color: #e9e9e9; -fx-border-width: 0 1 0 0; -fx-border-color: #e0e0e0;">
            <children>
               <Button fx:id="createWarehouse" prefHeight="50.0" prefWidth="120.0" styleClass="settingsBtn" text="创建">
                  <graphic>
                     <ImageView fitHeight="20.0" fitWidth="20.0">
                        <image>
                           <Image url="@icons/add.png" />
                        </image>
                     </ImageView>
                  </graphic>
               </Button>
               <Button fx:id="flush" prefHeight="50.0" prefWidth="120.0" styleClass="settingsBtn" text="刷新">
                  <graphic>
                     <ImageView fitHeight="25.0" fitWidth="25.0">
                        <image>
                           <Image url="@icons/flush.png" />
                        </image>
                     </ImageView>
                  </graphic>
               </Button>
            </children>
         </VBox>
         <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
            <children>
               <Label prefWidth="1.0E7" style="-fx-font-size: 25; -fx-font-weight: bold; -fx-padding: 10; -fx-background-color: #f5f5f5; -fx-border-color: #e0e0e0; -fx-border-width: 0 0 1 0;" text="Title" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS" />
               <Label style="-fx-padding: 15; -fx-font-size: 15;" text="" wrapText="true" VBox.vgrow="ALWAYS" />
               <ListView prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                  <VBox.margin>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </VBox.margin>
               </ListView>
            </children>
         </VBox>
      </children>
   </HBox>
</VBox>
