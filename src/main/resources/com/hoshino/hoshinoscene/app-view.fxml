   <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
      <children>
         <FlowPane fx:id="content" prefHeight="200.0" prefWidth="200.0" style="-fx-hgap: 10; -fx-vgap: 10;" HBox.hgrow="ALWAYS" />
